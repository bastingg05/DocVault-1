{
  "version": 2,
  "buildCommand": "cd frontend_react && npm ci && npm run build",
  "outputDirectory": "frontend_react/dist",
  "installCommand": "cd frontend_react && npm ci",
  "framework": "vite",
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NPM_CONFIG_IGNORE_OPTIONAL": "false"
  }
}
